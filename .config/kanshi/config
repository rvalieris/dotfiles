    output DP-1 position 0,0
    output eDP-1 position 2560,0
    exec ~/.config/sway/sway_tricks.py reload_bg
}

profile nomad {
    output eDP-1 position 0,0
    exec ~/.config/sway/sway_tricks.py reload_bg
}

